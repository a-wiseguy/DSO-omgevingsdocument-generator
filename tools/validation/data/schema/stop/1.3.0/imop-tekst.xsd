<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:tekst="https://standaarden.overheid.nl/stop/imop/tekst/"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           targetNamespace="https://standaarden.overheid.nl/stop/imop/tekst/"
           version="1.3.0">
   <xs:annotation>
      <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/goto.html?to=stop&amp;id=xsd:tekst">"Tekst-schema" van
      STOP.</xs:documentation>
      <xs:documentation>Dit schema definieert de "tekst"-elementen voor de
      STOP-modules</xs:documentation>
      <xs:documentation>versie 1.3.0</xs:documentation>
      <xs:documentation>Documentatie: https://koop.gitlab.io/STOP/standaard/1.3.0/</xs:documentation>
   </xs:annotation>
   <!--  ELEMENTEN -->
   <!-- ROOT REGELINGEN -->
   <xs:element name="RegelingKlassiek">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:RegelingKlassiek"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tekst:RegelingOpschrift"/>
            <xs:element minOccurs="0" ref="tekst:Aanhef"/>
            <xs:element minOccurs="1" ref="tekst:Lichaam"/>
            <xs:element minOccurs="0" ref="tekst:Sluiting"/>
            <xs:group ref="tekst:mgBijlagenVoorRegeling"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agSchemaVersie"/>
         <xs:attributeGroup ref="tekst:agComponentNieuweRegeling"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="RegelingCompact">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:RegelingCompact"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tekst:RegelingOpschrift"/>
            <xs:element ref="tekst:Lichaam"/>
            <xs:group ref="tekst:mgBijlagenVoorRegeling"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agSchemaVersie"/>
         <xs:attributeGroup ref="tekst:agComponentNieuweRegeling"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="RegelingTijdelijkdeel">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:RegelingTijdelijkdeel"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tekst:RegelingOpschrift"/>
            <xs:element name="Lichaam">
               <xs:annotation>
                  <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:RegelingTijdelijkdeel:Lichaam"/>
               </xs:annotation>
               <xs:complexType>
                  <xs:sequence>
                     <xs:element ref="tekst:Conditie"/>
                     <xs:group ref="tekst:mgLichaamStructuurTijdelijkdeel"/>
                  </xs:sequence>
                  <xs:attributeGroup ref="tekst:agAKN"/>
               </xs:complexType>
            </xs:element>
            <xs:group ref="tekst:mgBijlagenVoorRegeling"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agSchemaVersie"/>
         <xs:attributeGroup ref="tekst:agComponentNieuweRegeling"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="RegelingVrijetekst">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:RegelingVrijetekst"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tekst:RegelingOpschrift"/>
            <xs:element name="Lichaam">
               <xs:annotation>
                  <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:RegelingVrijetekst:Lichaam"/>
               </xs:annotation>
               <xs:complexType>
                  <xs:sequence>
                     <xs:choice maxOccurs="unbounded">
                        <xs:group ref="tekst:mgDivisieStructuur"/>
                     </xs:choice>
                  </xs:sequence>
                  <xs:attributeGroup ref="tekst:agAKN"/>
               </xs:complexType>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:Bijlage"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agSchemaVersie"/>
         <xs:attributeGroup ref="tekst:agComponentNieuweRegeling"/>
      </xs:complexType>
   </xs:element>
   <!-- EINDE ROOT REGELINGEN -->
   <!-- ROOT BESLUITEN -->
   <xs:element name="BesluitKlassiek">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:BesluitKlassiek"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="tekst:RegelingOpschrift"/>
            <xs:element minOccurs="0" ref="tekst:Aanhef"/>
            <xs:element ref="tekst:RegelingKlassiek"/>
            <xs:element minOccurs="0" ref="tekst:Sluiting"/>
            <xs:group ref="tekst:mgBijlagenVoorBesluit"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="BesluitCompact">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:BesluitCompact"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tekst:RegelingOpschrift"/>
            <xs:element minOccurs="0" ref="tekst:Aanhef"/>
            <xs:element ref="tekst:Lichaam"/>
            <xs:element minOccurs="0" ref="tekst:Sluiting"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:WijzigBijlage"/>
            <xs:group ref="tekst:mgBijlagenVoorBesluit"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      </xs:complexType>
   </xs:element>
   <!-- EINDE ROOT BESLUITEN -->
   <!-- BEGIN ROOT ANDERS -->
   <xs:element name="Kennisgeving">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Kennisgeving"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tekst:RegelingOpschrift"/>
            <xs:element name="Lichaam">
               <xs:annotation>
                  <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Kennisgeving:Lichaam"/>
               </xs:annotation>
               <xs:complexType>
                  <xs:choice maxOccurs="unbounded">
                     <xs:sequence>
                        <xs:group ref="tekst:mgDivisieStructuur"/>
                     </xs:sequence>
                  </xs:choice>
                  <xs:attributeGroup ref="tekst:agAKN"/>
               </xs:complexType>
            </xs:element>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Rectificatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Rectificatie"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tekst:RegelingOpschrift"/>
            <xs:element maxOccurs="1" minOccurs="1" name="Lichaam">
               <xs:annotation>
                  <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Rectificatie:Lichaam"/>
               </xs:annotation>
               <xs:complexType>
                  <xs:sequence>
                     <xs:element maxOccurs="unbounded" minOccurs="1" ref="tekst:Rectificatietekst"/>
                  </xs:sequence>
                  <xs:attributeGroup ref="tekst:agAKN"/>
               </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" ref="tekst:Sluiting"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Tekstrevisie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Tekstrevisie"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" ref="tekst:RegelingMutatie"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="OfficielePublicatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:OfficielePublicatie"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:group ref="tekst:mgBladen"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      </xs:complexType>
   </xs:element>
   <!-- EINDE ROOT -->
   <!-- PUBLICATIES -->
   <xs:element name="Staatscourant">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Staatscourant"/>
      </xs:annotation>
      <xs:complexType>
         <xs:group ref="tekst:mgBlad"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Staatsblad">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Staatsblad"/>
      </xs:annotation>
      <xs:complexType>
         <xs:group ref="tekst:mgBlad"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Gemeenteblad">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Gemeenteblad"/>
      </xs:annotation>
      <xs:complexType>
         <xs:group ref="tekst:mgBlad"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Provinciaalblad">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Provinciaalblad"/>
      </xs:annotation>
      <xs:complexType>
         <xs:group ref="tekst:mgBlad"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Waterschapsblad">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Waterschapsblad"/>
      </xs:annotation>
      <xs:complexType>
         <xs:group ref="tekst:mgBlad"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="BladGR">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:BladGR"/>
      </xs:annotation>
      <xs:complexType>
         <xs:group ref="tekst:mgBlad"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Bladaanduiding">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Bladaanduiding"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="2" ref="tekst:Titelregel"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Titelregel">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Titelregel"/>
      </xs:annotation>
      <xs:complexType mixed="true">
         <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:group ref="tekst:mgInlinesCompleet"/>
         </xs:choice>
         <xs:attributeGroup ref="tekst:agWijzigactie"/>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
      </xs:complexType>
   </xs:element>
   <!-- EINDE PUBLICATIES -->
   <!-- ELEMENTEN FORMEEL -->
   <xs:element name="Bijlage">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Bijlage"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tekst:Kop"/>
            <xs:choice maxOccurs="1" minOccurs="0">
               <xs:element maxOccurs="1" ref="tekst:Gereserveerd"/>
               <xs:element maxOccurs="1" ref="tekst:Vervallen"/>
            </xs:choice>
            <xs:sequence>
               <xs:element maxOccurs="1" minOccurs="0" ref="tekst:InleidendeTekst"/>
               <xs:choice maxOccurs="unbounded" minOccurs="0">
                  <xs:element ref="tekst:Divisie"/>
                  <xs:element ref="tekst:Divisietekst"/>
               </xs:choice>
            </xs:sequence>
            <xs:element minOccurs="0" ref="tekst:Sluiting"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agAKN"/>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Toelichting">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Toelichting"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Kop"/>
            <xs:choice>
               <xs:sequence>
                  <xs:element maxOccurs="1" minOccurs="0" ref="tekst:InleidendeTekst"/>
                  <xs:element maxOccurs="1" minOccurs="0" ref="tekst:AlgemeneToelichting"/>
                  <xs:element maxOccurs="1" minOccurs="0" ref="tekst:ArtikelgewijzeToelichting"/>
               </xs:sequence>
               <xs:choice maxOccurs="unbounded" minOccurs="1">
                  <xs:element ref="tekst:Divisie"/>
                  <xs:element ref="tekst:Divisietekst"/>
               </xs:choice>
            </xs:choice>
            <xs:element minOccurs="0" ref="tekst:Sluiting"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:Bijlage"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agAKN"/>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="AlgemeneToelichting">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:AlgemeneToelichting"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="1" ref="tekst:Kop"/>
            <xs:sequence>
               <xs:element maxOccurs="1" minOccurs="0" ref="tekst:InleidendeTekst"/>
               <xs:choice maxOccurs="unbounded" minOccurs="1">
                  <xs:element ref="tekst:Divisie"/>
                  <xs:element ref="tekst:Divisietekst"/>
               </xs:choice>
            </xs:sequence>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agAKN"/>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="ArtikelgewijzeToelichting">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:ArtikelgewijzeToelichting"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="1" ref="tekst:Kop"/>
            <xs:choice>
               <xs:sequence>
                  <xs:element maxOccurs="1" minOccurs="0" ref="tekst:InleidendeTekst"/>
                  <xs:choice maxOccurs="unbounded" minOccurs="1">
                     <xs:element ref="tekst:Divisie"/>
                     <xs:element ref="tekst:Divisietekst"/>
                  </xs:choice>
               </xs:sequence>
            </xs:choice>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agAKN"/>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Conditie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Conditie"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" ref="tekst:Artikel"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Divisietekst">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Divisietekst"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Kop"/>
            <xs:choice maxOccurs="1" minOccurs="0">
               <xs:element maxOccurs="1" ref="tekst:Gereserveerd"/>
               <xs:element maxOccurs="1" ref="tekst:Vervallen"/>
            </xs:choice>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Inhoud"/>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agAKN"/>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
         <xs:attributeGroup ref="tekst:agWijzigactie"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Divisie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Divisie"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="1" ref="tekst:Kop"/>
            <xs:choice maxOccurs="1" minOccurs="0">
               <xs:element maxOccurs="1" ref="tekst:Gereserveerd"/>
               <xs:element maxOccurs="1" ref="tekst:Vervallen"/>
            </xs:choice>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:InleidendeTekst"/>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
               <xs:element ref="tekst:Divisie"/>
               <xs:element ref="tekst:Divisietekst"/>
            </xs:choice>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agAKN"/>
         <xs:attributeGroup ref="tekst:agWijzigactie"/>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="InleidendeTekst">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:InleidendeTekst"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Kop"/>
            <xs:choice maxOccurs="unbounded" minOccurs="1">
               <xs:element ref="tekst:Al"/>
               <xs:element ref="tekst:Lijst"/>
               <xs:element ref="tekst:Figuur"/>
               <xs:element ref="tekst:Tussenkop"/>
            </xs:choice>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
         <xs:attributeGroup ref="tekst:agWijzigactie"/>
         <xs:attributeGroup ref="tekst:agAKN"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Inhoud">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Inhoud"/>
      </xs:annotation>
      <xs:complexType>
         <xs:choice maxOccurs="unbounded" minOccurs="1">
            <xs:group ref="tekst:mgBlocksMinimaal"/>
            <xs:group ref="tekst:mgBlocksVolledig"/>
            <xs:element ref="tekst:Kadertekst"/>
         </xs:choice>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
         <xs:attributeGroup ref="tekst:agWijzigactie"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Kadertekst">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Kadertekst"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Kop"/>
            <xs:choice maxOccurs="unbounded" minOccurs="1">
               <xs:group ref="tekst:mgBlocksMinimaal"/>
               <xs:group ref="tekst:mgBlocksVolledig"/>
            </xs:choice>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agWijzigactie"/>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
         <xs:attributeGroup ref="tekst:agAKN"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Gereserveerd">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Gereserveerd"/>
      </xs:annotation>
      <xs:complexType>
         <xs:attributeGroup ref="tekst:agWijzigactie"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Vervallen">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:Vervallen"/>
      </xs:annotation>
      <xs:complexType>
         <xs:attribute name="wijzigactie" type="tekst:wlWijzigactieBeperkt" use="optional"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="WijzigBijlage">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:WijzigBijlage"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Kop"/>
            <xs:choice>
               <xs:element ref="tekst:RegelingMutatie"/>
               <xs:group ref="tekst:mgRegelingModellen"/>
            </xs:choice>
         </xs:sequence>
         <xs:attributeGroup ref="tekst:agAKN"/>
         <xs:attributeGroup ref="tekst:agAlgemeen"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="RegelingMutatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:RegelingMutatie"/>
      </xs:annotation>
      <xs:complexType>
         <xs:choice maxOccurs="unbounded">
            <xs:element ref="tekst:VoegToe"/>
            <xs:element ref="tekst:Vervang"/>
            <xs:element ref="tekst:VervangKop"/>
            <xs:element ref="tekst:Verwijder"/>
            <xs:element ref="tekst:VervangRegeling"/>
         </xs:choice>
         <xs:attributeGroup ref="tekst:agComponentMutatie"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="VervangRegeling">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:tekst:VervangRegeling"/>
